<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FinalProjectAcad" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1b5e20</item>
        <item name="colorPrimaryVariant">#d4e157</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#a5d6a7</item>
        <item name="colorSecondaryVariant">#dcedc8</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:textColorHint">#757575</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>